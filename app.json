{
  "name": "navve.download",
  "description": "HTML GUI for youtube-dl",
  "repository": "https://github.com/aloproducao/alltube",
  "logo": "https://alltubedownload.net/img/logo.png",
  "keywords": [
    "alltube",
    "download",
    "video",
    "youtube",
    "php",
    "youtubedownload",
    "baixar do youtube",
    "navve", "navve.studio",
    "navve.tv"
  ],
  "buildpacks": [
    {
      "url": "heroku/php"
    },
    {
      "url": "heroku/python"
    }
  ],
  "env": {
    "PYTHON": {
      "description": "Path to python binary",
      "value": "/app/.heroku/python/bin/python"
    },
    "STREAM": {
      "description": "Enable stream mode",
      "value": "true",
      "required": false
    }
  },
  "website": "http://download.navve.studio/"
}
